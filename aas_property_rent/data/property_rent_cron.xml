<odoo>
    <data noupdate="1">
        <record id="property_rent_check_renewal_cron" model="ir.cron">
            <field name="name">Rent Renewal Check</field>
            <field name="model_id" ref="model_property_rent" />
            <field name="state">code</field>
            <field name="code">model.cron_check_renewal()</field>
            <field name="user_id" ref="base.user_root" />
            <field name='interval_number'>1</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
        </record>

        <record id="property_check_auto_renewal_cron" model="ir.cron">
            <field name="name">Auto Renewal Check</field>
            <field name="model_id" ref="model_property_rent" />
            <field name="state">code</field>
            <field name="code">model.cron_check_auto_renewal()</field>
            <field name="user_id" ref="base.user_root" />
            <!-- <field name="nextcall" eval="(DateTime.now()+timedelta(days=1)).strftime('%Y-%m-%d 12:01:00')" /> -->
            <field name='interval_number'>1</field>
            <field name='interval_type'>minutes</field>
            <!-- <field name='interval_type'>days</field> -->
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
        </record>
    </data>
</odoo>